title: Living Room
widget_dimensions: [97, 93]
widget_size: [1, 1]
widget_margins: [2, 2]
columns: 8
global_parameters:
  use_comma: 0
  precision: 1
  use_hass_icon: 1

layout:
  - cam_foscam_c1(6x4), light.living_room_table_light, light.living_room_tv_light
  - scene.living_room_normal, scene.living_room_dim
  - media_player.living_room_tv(2x2)
  - empty: 1
  - include: rpi_bottom_panel

