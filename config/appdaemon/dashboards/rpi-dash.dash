title: CCTV Panel
widget_dimensions: [97, 93]
widget_size: [1, 1]
widget_margins: [2, 2]
columns: 8
global_parameters:
  use_comma: 0
  precision: 1
  use_hass_icon: 1
  state_text: 0


layout:
  - cam_foscam_c1(3x2), cam_foscam_c2(3x2)
  - empty: 2
  - cam_hassistant(3x2), cam_doorpi(3x2)
  - empty: 2
  - cam_winpi(3x2)
  - empty: 2
  - include: bottom_panel

